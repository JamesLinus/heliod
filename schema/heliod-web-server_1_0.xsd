<xs:simpleType name="ipAddressType">
  <xs:annotation>
    <xs:documentation>
      The value must be an IP address.
    </xs:documentation>
  </xs:annotation>
  <xs:union memberTypes="ipv4AddressType ipv6AddressType"/>
</xs:simpleType>

<xs:simpleType name="hostType">
  <xs:annotation>
    <xs:documentation>
      The value must be a hostname or IP address.
    </xs:documentation>
  </xs:annotation>
  <xs:union memberTypes="hostnameType ipAddressType"/>
</xs:simpleType>

<xs:simpleType name="hostPatternType">
  <xs:annotation>
    <xs:documentation>
      The value must be a wildcard pattern that matches one or more hostnames.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="[\-a-zA-Z0-9:.*?(|)$\[\]\^]+"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="listenAddressType">
  <xs:annotation>
    <xs:documentation>
      The value must be *, a hostname, or an IP address.
    </xs:documentation>
  </xs:annotation>
  <xs:union memberTypes="hostType">
    <xs:simpleType>
      <xs:restriction base="xs:token">
        <xs:enumeration value="*"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:union>
</xs:simpleType>

<xs:simpleType name="portType">
  <xs:annotation>
    <xs:documentation>
      The value must be a port number between 1 and 65535, inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:integer">
    <xs:minInclusive value="1"/>
    <xs:maxInclusive value="65535"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="serverNameType">
  <xs:annotation>
    <xs:documentation>
      The value must be a hostname, fully qualified domain name, IP address, or an URL prefix that contains one. The URL prefix must not specify a path.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="([a-zA-Z][\-a-zA-Z0-9]*://|)[^/\s]+(/|)"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="urlType">
  <xs:annotation>
    <xs:documentation>
      The value must be a URL.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="[a-zA-Z][\-a-zA-Z0-9]*(://.*)?"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="familyType">
  <xs:annotation>
    <xs:documentation>
      The value must be default, inet, inet6, or nca.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="default"/>
    <xs:enumeration value="inet"/>
    <xs:enumeration value="inet6"/>
    <xs:enumeration value="nca"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="authRealmTypeType">
  <xs:annotation>
    <xs:documentation>
      The value must be file, ldap, pam, certificate, or native.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="file"/>
    <xs:enumeration value="ldap"/>
    <xs:enumeration value="pam"/>
    <xs:enumeration value="certificate"/>
    <xs:enumeration value="native"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="logLevelType">
  <xs:annotation>
    <xs:documentation>
      The value must be a log level. Valid log levels are finest, finer, fine, info, warning, failure, config, security, and catastrophe.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="catastrophe">
      <xs:annotation>
        <xs:documentation>
          catastrophe log messages record fatal errors.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="security">
      <xs:annotation>
        <xs:documentation>
          security log messages record security issues.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="config">
      <xs:annotation>
        <xs:documentation>
          config log messages record misconfigurations.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="failure">
      <xs:annotation>
        <xs:documentation>
          failure log messages record failures.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="warning">
      <xs:annotation>
        <xs:documentation>
          warning log messages contain warnings.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="info">
      <xs:annotation>
        <xs:documentation>
          info log messages contain informational messages.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="fine">
      <xs:annotation>
        <xs:documentation>
          fine log messages contain debug or diagnostic messages.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="finer">
      <xs:annotation>
        <xs:documentation>
          finer log messages contain debug or diagnostic messages.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="finest">
      <xs:annotation>
        <xs:documentation>
          finest log messages contain debug or diagnostic messages.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="access-logModeType">
  <xs:annotation>
    <xs:documentation>
      The value must be a log mode. Valid log modes are text and binary.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="text">
      <xs:annotation>
        <xs:documentation>
          Use text format for access log.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="binary">
      <xs:annotation>
        <xs:documentation>
          Use binary format for access log.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="sizeType">
  <xs:annotation>
    <xs:documentation>
      The value must be a size in bytes between 0 and 2147483647, inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:integer">
    <xs:minInclusive value="0"/>
    <xs:maxInclusive value="2147483647"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="stackSizeType">
  <xs:annotation>
    <xs:documentation>
      The value must be a stack size in bytes between 8192 and 268435456, inclusive. 0 indicates the platform-specific default stack size should be used.
    </xs:documentation>
  </xs:annotation>
  <xs:union>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="8192"/>
        <xs:maxInclusive value="268435456"/>
      </xs:restriction>
    </xs:simpleType>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="0"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:union>
</xs:simpleType>

<xs:simpleType name="maxSizeType">
  <xs:annotation>
    <xs:documentation>
      The value must be a maximum size in bytes between 0 and 2147483647, inclusive. -1 indicates there is no maximum size.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:integer">
    <xs:minInclusive value="-1"/>
    <xs:maxInclusive value="2147483647"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="maxAddressSpaceType">
  <xs:annotation>
    <xs:documentation>
      The value must be a maximum amount of address space in bytes between 0 and 1099511627776, inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:integer">
    <xs:minInclusive value="0"/>
    <xs:maxInclusive value="1099511627776"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="timeoutType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 0 and 3600 (1 hour), inclusive. -1 indicates no timeout.
    </xs:documentation>
  </xs:annotation>
  <xs:union>
    <xs:simpleType>
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="3600"/>
        <xs:fractionDigits value="3"/>
      </xs:restriction>
    </xs:simpleType>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="-1"/>
        <xs:maxInclusive value="-1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:union>
</xs:simpleType>

<xs:simpleType name="nonZeroTimeoutType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 0.001 (1 millisecond) and 3600 (1 hour), inclusive. -1 indicates no timeout.
    </xs:documentation>
  </xs:annotation>
  <xs:union>
    <xs:simpleType>
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.001"/>
        <xs:maxInclusive value="3600"/>
        <xs:fractionDigits value="3"/>
      </xs:restriction>
    </xs:simpleType>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="-1"/>
        <xs:maxInclusive value="-1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:union>
</xs:simpleType>

<xs:simpleType name="largeTimeoutType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 0 and 604800 (1 week), inclusive. -1 indicates no timeout.
    </xs:documentation>
  </xs:annotation>
  <xs:union>
    <xs:simpleType>
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="604800"/>
        <xs:fractionDigits value="3"/>
      </xs:restriction>
    </xs:simpleType>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="-1"/>
        <xs:maxInclusive value="-1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:union>
</xs:simpleType>

<xs:simpleType name="intervalType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 0.001 and 3600 (1 hour), inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:decimal">
    <xs:minInclusive value="0.001"/>
    <xs:maxInclusive value="3600"/>
    <xs:fractionDigits value="3"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="largeIntervalType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 0.001 (1 millisecond) and 604800 (1 week), inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:decimal">
    <xs:minInclusive value="0.001"/>
    <xs:maxInclusive value="604800"/>
    <xs:fractionDigits value="3"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="eventIntervalType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 60 (1 minute) and 86400 (1 day), inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:integer">
    <xs:minInclusive value="60"/>
    <xs:maxInclusive value="86400"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="pollIntervalType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 0.001 and 1, inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:decimal">
    <xs:minInclusive value="0.001"/>
    <xs:maxInclusive value="1"/>
    <xs:fractionDigits value="3"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="cacheUpdateIntervalType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 0.001 and 3600 (1 hour), inclusive. 0 disables caching.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:decimal">
    <xs:minInclusive value="0"/>
    <xs:maxInclusive value="3600"/>
    <xs:fractionDigits value="3"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="dynamicReloadIntervalType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 1 and 60, inclusive. -1 disables dynamic reloading.
    </xs:documentation>
  </xs:annotation>
  <xs:union>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="60"/>
      </xs:restriction>
    </xs:simpleType>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="-1"/>
        <xs:maxInclusive value="-1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:union>
</xs:simpleType>

<xs:simpleType name="minLockTimeoutType">
  <xs:annotation>
    <xs:documentation>
      The value must be an interval in seconds between 0.001 and 3600 (1 hour), inclusive. 0 indicates there is no minimum lock timeout.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:decimal">
    <xs:minInclusive value="0"/>
    <xs:maxInclusive value="3600"/>
    <xs:fractionDigits value="3"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="maxPropfindDepthType">
  <xs:annotation>
    <xs:documentation>
      The value must be one of 0, 1, or infinity.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="0"/>
    <xs:enumeration value="1"/>
    <xs:enumeration value="infinity"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="clientAuthType">
  <xs:annotation>
    <xs:documentation>
      The value must be one of required, optional, or false.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="required"/>
    <xs:enumeration value="optional"/>
    <xs:enumeration value="false"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="authMethodType">
  <xs:annotation>
    <xs:documentation>
      The value must be an authentication method. Valid authentication methods are basic, digest, and ssl.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="basic">
      <xs:annotation>
        <xs:documentation>
          Use HTTP basic authentication.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="digest">
      <xs:annotation>
        <xs:documentation>
          Use HTTP digest authentication.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="ssl">
      <xs:annotation>
        <xs:documentation>
          Use SSL/TLS client certificate authentication.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="roleType">
  <xs:annotation>
    <xs:documentation>
      The value must be a role name.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:minLength value="1"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="messageSourcePolicyType">
  <xs:annotation>
    <xs:documentation>
      The value must be a web services message sender authentication requirement. Valid sender authentication requirements are sender and content.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="sender"/>
    <xs:enumeration value="content"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="messageRecipientPolicyType">
  <xs:annotation>
    <xs:documentation>
      The value must be a web services message recipient authentication requirement. Valid recipient authentication requirements are before-content and after-content.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="before-content"/>
    <xs:enumeration value="after-content"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="jndiNameType">
  <xs:annotation>
    <xs:documentation>
      The value must be a JNDI name.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="noWhitespaceTokenType"/>
</xs:simpleType>

<xs:simpleType name="jdbcIsolationLevelType">
  <xs:annotation>
    <xs:documentation>
      The value must be a JDBC transaction isolation level. Valid isolation levels are default, read-uncommitted, read-committed, repeatable-read, and serializable.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="default"/>
    <xs:enumeration value="read-uncommitted"/>
    <xs:enumeration value="read-committed"/>
    <xs:enumeration value="repeatable-read"/>
    <xs:enumeration value="serializable"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="jdbcConnectionValidationType">
  <xs:annotation>
    <xs:documentation>
      The value must be false or a JDBC connection validation method. Valid methods are auto-commit, meta-data, and table.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="false"/>
    <xs:enumeration value="auto-commit"/>
    <xs:enumeration value="meta-data"/>
    <xs:enumeration value="table"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="secureSessionCookieType">
  <xs:annotation>
    <xs:documentation>
      The value must be one of dynamic, true or false.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="dynamic">
      <xs:annotation>
        <xs:documentation>
          The JSESSIONID cookie is marked secure, only if the request was received on a secure connection.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="true">
      <xs:annotation>
        <xs:documentation>
          The JSESSIONID cookie is always marked secure, even if the request was received on an unsecure connection.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="false">
      <xs:annotation>
        <xs:documentation>
          The JSESSIONID cookie is never marked secure, even if the request was received on a secure connection.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="timeOfDayType">
  <xs:annotation>
    <xs:documentation>
      The value must be a time of day in 24-hour format between 0:00 and 23:59, inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="(0?[0-9]|1[0-9]|2[0-3]):[0-5][0-9]"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="dayOfWeekType">
  <xs:annotation>
    <xs:documentation>
      The value must be a token representing a day of the week. Valid tokens are Sun, Mon, Tue, Wed, Thu, Fri, and Sat.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="Sun"/>
    <xs:enumeration value="Mon"/>
    <xs:enumeration value="Tue"/>
    <xs:enumeration value="Wed"/>
    <xs:enumeration value="Thu"/>
    <xs:enumeration value="Fri"/>
    <xs:enumeration value="Sat"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="dayOfMonthType">
  <xs:annotation>
    <xs:documentation>
      The value must be a day of the month between 1 and 31, inclusive.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:integer">
    <xs:minInclusive value="1"/>
    <xs:maxInclusive value="31"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="monthType">
  <xs:annotation>
    <xs:documentation>
      The value must be a token representing a month. Valid tokens are Jan, Feb, Mar, Apr, May, Jun, Jul, Aug, Sep, Oct, Nov, and Dec.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="Jan"/>
    <xs:enumeration value="Feb"/>
    <xs:enumeration value="Mar"/>
    <xs:enumeration value="Apr"/>
    <xs:enumeration value="May"/>
    <xs:enumeration value="Jun"/>
    <xs:enumeration value="Jul"/>
    <xs:enumeration value="Aug"/>
    <xs:enumeration value="Sep"/>
    <xs:enumeration value="Oct"/>
    <xs:enumeration value="Nov"/>
    <xs:enumeration value="Dec"/>
  </xs:restriction>
</xs:simpleType>

</xs:schema>
