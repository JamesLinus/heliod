    <xs:documentation>
      The value must be a user name.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:minLength value="1"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="emailType">
  <xs:annotation>
    <xs:documentation>
      The value must be an email address.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="[^\s:@/]+(@([a-zA-Z0-9]([\-a-zA-Z0-9]*[a-zA-Z0-9]|)\.)*[a-zA-Z]([\-a-zA-Z0-9]*[a-zA-Z0-9]|)|)"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="pathType">
  <xs:annotation>
    <xs:documentation>
      The value must be a pathname.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:string">
    <xs:minLength value="1"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="pathPatternType">
  <xs:annotation>
    <xs:documentation>
      The value must be a wildcard pattern that matches one or more pathnames.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:minLength value="1"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="classPathType">
  <xs:annotation>
    <xs:documentation>
      The value must be a class path.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:string"/>
</xs:simpleType>

<xs:simpleType name="libraryPathType">
  <xs:annotation>
    <xs:documentation>
      The value must be a library path.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:string"/>
</xs:simpleType>

<xs:simpleType name="uriType">
  <xs:annotation>
    <xs:documentation>
      The value must be a URI (virtual directory) that begins with /.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:string">
    <xs:pattern value="/([^/](/|))*"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="fileExtensionType">
  <xs:annotation>
    <xs:documentation>
      The value must be a file extension without the leading dot.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="[^\.][^/]*"/>
    <xs:minLength value="0"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="variableNameType">
  <xs:annotation>
    <xs:documentation>
      The value must be a variable name consisting of letters, numbers, and underscores. Variable names must not begin with a number.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="[_a-zA-Z][_a-zA-Z0-9]*"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="noWhitespaceTokenType">
  <xs:annotation>
    <xs:documentation>
      The value must be a token consisting of one or more non-whitespace characters.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="\S+"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="objectType">
  <xs:annotation>
    <xs:documentation>
      The value must be an NSAPI Object name.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:string">
    <xs:minLength value="1"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="nameType">
  <xs:annotation>
    <xs:documentation>
      The value must be a name consisting of one or more characters. Whitespace is not permitted.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="noWhitespaceTokenType"/>
</xs:simpleType>

<xs:simpleType name="classType">
  <xs:annotation>
    <xs:documentation>
      The value must be a fully qualified class name.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="noWhitespaceTokenType">
    <xs:pattern value="[^\s\./]+(\.[^\s\./]+)+"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="cipherType">
  <xs:annotation>
    <xs:documentation>
      The value must specify a JCE cipher. JCE ciphers are specified using the form algorithm/mode/padding.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:string">
    <xs:pattern value="[^/]+/[^/]+/[^/]+"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="sessionReplicationProtocolType">
  <xs:annotation>
    <xs:documentation>
      The value must be a session replication protocol. Valid session replication protocols are HTTP and JRMP.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:enumeration value="HTTP">
      <xs:annotation>
        <xs:documentation>
          Use Hypertext Transfer Protocol for session replication.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
    <xs:enumeration value="JRMP">
      <xs:annotation>
        <xs:documentation>
          Use Java Remote Method Protocol for session replication.
        </xs:documentation>
      </xs:annotation>
    </xs:enumeration>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="hostnameType">
  <xs:annotation>
    <xs:documentation>
      The value must be a hostname. Hostnames consist of one or more domain name labels separated by periods. Domain name labels may contain letters, numbers, and hyphens and must not begin or end with a hyphen. The terminal domain name label must not begin with a number.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="([a-zA-Z0-9]([\-a-zA-Z0-9]*[a-zA-Z0-9]|)\.)*[a-zA-Z]([\-a-zA-Z0-9]*[a-zA-Z0-9]|)"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="ipv4AddressType">
  <xs:annotation>
    <xs:documentation>
      The value must be an IPv4 address in dotted decimal notation with no leading zeros.
    </xs:documentation>
  </xs:annotation>
  <xs:restriction base="xs:token">
    <xs:pattern value="([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(\.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="ipv6AddressType">
  <xs:annotation>
    <xs:documentation>
      The conventional forms for IPv6 addresses are described in RFC 3513 section 2.2.
    </xs:documentation>
