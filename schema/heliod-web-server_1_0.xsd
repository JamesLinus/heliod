<xs:element name="server">
  <xs:annotation>
    <xs:documentation>
      The &lt;server> element is the root element of all server.xml documents.
    </xs:documentation>
  </xs:annotation>

  <xs:complexType>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="default-auth-db-name" type="nameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <appinfo:implicit>default</appinfo:implicit>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="default-auth-realm-name" type="nameType" minOccurs="0" maxOccurs="1"/>

      <xs:element name="default-soap-auth-provider-name" type="nameType" minOccurs="0" maxOccurs="1"/>

      <xs:element name="cluster" type="clusterType" minOccurs="0" maxOccurs="1">
        <xs:key name="instance-hostKey">
          <xs:annotation>
            <xs:documentation>
              Each &lt;instance> must specify a unique host.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="instance"/>
          <xs:field xpath="host"/>
        </xs:key>

        <xs:keyref name="local-hostKeyref" refer="instance-hostKey">
          <xs:annotation>
            <xs:documentation>
              &lt;local-host> must refer to an existing &lt;instance>.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="."/>
          <xs:field xpath="local-host"/>
        </xs:keyref>
      </xs:element>

      <xs:element name="log" type="logType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;log> element is omitted, logging settings are configured implicitly.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="platform" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;platform> element is omitted, the server runs in 32-bit mode.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit>32</appinfo:implicit>
          </xs:appinfo>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="32"/>
            <xs:enumeration value="64"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
          
      <xs:element name="user" type="userType" minOccurs="0" maxOccurs="1"/>

      <xs:element name="temp-path" type="pathType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;temp-path> element is omitted, a platform-dependent temporary directory is used.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="variable" type="variableType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="localization" type="localizationType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;localization> element is omitted, localization settings are configured implicitly.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="http" type="httpType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;http> element is omitted, HTTP protocol settings are configured implicitly.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="config-file" type="pathPatternType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="keep-alive" type="keep-aliveType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;keep-alive> element is omitted, the HTTP keep-alive subsystem is implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="thread-pool" type="thread-poolType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;thread-pool> element is omitted, thread pool settings are configured implicitly.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="pkcs11" type="pkcs11Type" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;pkcs11> element is omitted and one or more &lt;ssl> elements are enabled or if one or more &lt;soap-auth-provider> elements are defined, PKCS #11 support is implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit if-xpath="(/server/soap-auth-provider|/server/http-listener/ssl|/server/auth-db/property[name = 'client-cert-nickname'])[not(ancestor-or-self::*[enabled = 'false'])]"/>
            <appinfo:implicit>
              <enabled>false</enabled>
            </appinfo:implicit>
          </xs:appinfo>
        </xs:annotation>

        <xs:unique name="pkcs11-token-nameUnique">
          <xs:annotation>
            <xs:documentation>
              Each &lt;token> must specify a unique name.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="token"/>
          <xs:field xpath="name"/>
        </xs:unique>
      </xs:element>

      <xs:element name="stats" type="statsType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;stats> element is omitted, statistics collection is implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="cgi" type="cgiType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;cgi> element is omitted, CGI subsystem settings are configured implicitly.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="qos" type="qosType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;qos> element is omitted and one or more &lt;qos-limits> elements are enabled, QOS features are implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit if-xpath="(/server/qos-limits|/server/virtual-server/qos-limits)[not(ancestor-or-self::*[enabled = 'false'])]"/>
            <appinfo:implicit>
              <enabled>false</enabled>
            </appinfo:implicit>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="dns" type="dnsType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;dns> element is omitted, DNS lookups are disabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit>
              <enabled>false</enabled>
            </appinfo:implicit>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="dns-cache" type="dns-cacheType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;dns-cache> element is omitted, the DNS cache is implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="file-cache" type="file-cacheType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;file-cache> element is omitted, the file cache is implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="acl-cache" type="acl-cacheType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;acl-cache> element is omitted, the ACL cache is implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="ssl-session-cache" type="ssl-session-cacheType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;ssl-session-cache> element is omitted, the SSL/TLS session cache is implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="access-log-buffer" type="access-log-bufferType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;access-log-buffer> element is omitted, access log buffering is implicitly enabled.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="dav" type="davType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;dav> element is omitted, WebDAV is configured implicitly.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="snmp" type="snmpType" minOccurs="0" maxOccurs="1"/>

      <xs:element name="qos-limits" type="qos-limitsType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;qos-limits> element is omitted, no QOS limits are enforced.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit>
              <enabled>false</enabled>
            </appinfo:implicit>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="audit-accesses" type="xs:boolean" default="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <appinfo:implicit>false</appinfo:implicit>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="jvm" type="jvmType" minOccurs="0" maxOccurs="1"/>

      <xs:element name="servlet-container" type="servlet-containerType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            If the &lt;servlet-container> element is omitted and the JVM is enabled, the Servlet container is configured implicitly.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit if-xpath="/server/jvm[not(ancestor-or-self::*[enabled = 'false'])]"/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="lifecycle-module" type="lifecycle-moduleType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="custom-resource" type="custom-resourceType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="external-jndi-resource" type="external-jndi-resourceType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="jdbc-resource" type="jdbc-resourceType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="mail-resource" type="mail-resourceType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="soap-auth-provider" type="soap-auth-providerType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="auth-realm" type="auth-realmType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="auth-db" type="auth-dbType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="acl-file" type="pathType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="krb5-keytab-file" type="pathType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Sets the value of the KRB5_KTNAME environment variable.
          </xs:documentation>
          <xs:appinfo>
            <appinfo:implicit>keytab</appinfo:implicit>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="mime-file" type="pathType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="access-log" type="access-logType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="http-listener" type="http-listenerType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element name="virtual-server" type="virtual-serverType" minOccurs="0" maxOccurs="unbounded">
        <xs:unique name="virtual-server-variable-nameUnique">
          <xs:annotation>
            <xs:documentation>
              Each &lt;variable> must specify a unique name.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="variable"/>
          <xs:field xpath="name"/>
        </xs:unique>

        <xs:unique name="virtual-server-auth-db-nameUnique">
          <xs:annotation>
            <xs:documentation>
              Each &lt;auth-db> must specify a unique name.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="auth-db"/>
          <xs:field xpath="name"/>
        </xs:unique>

        <xs:unique name="virtual-server-access-log-nameUnique">
          <xs:annotation>
            <xs:documentation>
              Each &lt;access-log> must specify a unique name.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="access-log"/>
          <xs:field xpath="name"/>
        </xs:unique>

        <xs:unique name="virtual-server-search-collection-nameUnique">
          <xs:annotation>
            <xs:documentation>
              Each &lt;search-collection> must specify a unique name.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="search-collection"/>
          <xs:field xpath="name"/>
        </xs:unique>
      </xs:element>

      <xs:element name="event" type="eventType" minOccurs="0" maxOccurs="unbounded"/>

      <xs:element ref="server-extension-element" minOccurs="0" maxOccurs="unbounded"/>
    </xs:choice>
  </xs:complexType>

  <xs:unique name="server-variable-nameUnique">
    <xs:annotation>
      <xs:documentation>
        Each &lt;variable> must specify a unique name.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="variable"/>
    <xs:field xpath="name"/>
  </xs:unique>

  <xs:unique name="server-lifecycle-module-nameUnique">
    <xs:annotation>
      <xs:documentation>
        Each &lt;lifecycle-module> must specify a unique name.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="lifecycle-module"/>
    <xs:field xpath="name"/>
  </xs:unique>

  <xs:unique name="server-auth-db-nameUnique">
    <xs:annotation>
      <xs:documentation>
        Each &lt;auth-db> must specify a unique name.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="auth-db"/>
    <xs:field xpath="name"/>
  </xs:unique>

  <xs:unique name="server-access-log-nameUnique">
    <xs:annotation>
      <xs:documentation>
        Each &lt;access-log> must specify a unique name.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="access-log"/>
    <xs:field xpath="name"/>
  </xs:unique>

  <xs:key name="soap-auth-provider-nameKey">
    <xs:annotation>
      <xs:documentation>
        Each &lt;soap-auth-provider> must specify a unique name.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="soap-auth-provider"/>
    <xs:field xpath="name"/>
  </xs:key>

  <xs:key name="auth-realm-nameKey">
    <xs:annotation>
      <xs:documentation>
        Each &lt;auth-realm> must specify a unique name.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="auth-realm"/>
    <xs:field xpath="name"/>
  </xs:key>

  <xs:key name="http-listener-nameKey">
    <xs:annotation>
      <xs:documentation>
        Each &lt;http-listener> must specify a unique name.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="http-listener"/>
    <xs:field xpath="name"/>
  </xs:key>

  <xs:key name="virtual-server-nameKey">
    <xs:annotation>
      <xs:documentation>
        Each &lt;virtual-server> must specify a unique name.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="virtual-server"/>
    <xs:field xpath="name"/>
  </xs:key>

  <xs:keyref name="default-soap-auth-provider-nameKeyref" refer="soap-auth-provider-nameKey">
    <xs:annotation>
      <xs:documentation>
        &lt;default-soap-auth-provider-name> must refer to an existing &lt;soap-auth-provider>.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="."/>
    <xs:field xpath="default-soap-auth-provider-name"/>
  </xs:keyref>

  <xs:keyref name="default-auth-realm-nameKeyref" refer="auth-realm-nameKey">
    <xs:annotation>
      <xs:documentation>
        &lt;default-auth-realm-name> must refer to an existing &lt;auth-realm>.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="."/>
    <xs:field xpath="default-auth-realm-name"/>
  </xs:keyref>

  <xs:keyref name="default-virtual-server-nameKeyref" refer="virtual-server-nameKey">
    <xs:annotation>
      <xs:documentation>
        &lt;default-virtual-server-name> must refer to an existing &lt;virtual-server>.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="http-listener"/>
    <xs:field xpath="default-virtual-server-name"/>
  </xs:keyref>

  <xs:keyref name="http-listener-nameKeyref" refer="http-listener-nameKey">
    <xs:annotation>
      <xs:documentation>
        &lt;http-listener-name> must refer to an existing &lt;http-listener>.
      </xs:documentation>
    </xs:annotation>
    <xs:selector xpath="virtual-server"/>
    <xs:field xpath="http-listener-name"/>
  </xs:keyref>
</xs:element>

<xs:element name="server-extension-element" type="server-extension-elementType" abstract="true"/>

<xs:element name="virtual-server-extension-element" type="virtual-server-extension-elementType" abstract="true"/>

<xs:complexType name="server-extension-elementType"/>

<xs:complexType name="clusterType">
  <xs:choice minOccurs="0" maxOccurs="unbounded">
    <xs:element name="local-host" type="hostType" minOccurs="1" maxOccurs="1"/>

    <xs:element name="instance" type="instanceType" minOccurs="1" maxOccurs="unbounded"/>

    <xs:element name="session-replication" type="cluster-session-replicationType" minOccurs="0" maxOccurs="1">
      <xs:annotation>
        <xs:documentation>
          If the &lt;session-replication> element is omitted, session-replication support is disabled.
        </xs:documentation>
        <xs:appinfo>
          <appinfo:implicit>
            <enabled>false</enabled>
          </appinfo:implicit>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>
  </xs:choice>
</xs:complexType>

<xs:complexType name="instanceType">
  <xs:all>
    <xs:element name="enabled" type="xs:boolean" default="true" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit/>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="host" type="hostType"/>

    <xs:element name="session-replication" type="instance-session-replicationType" minOccurs="0"/>
  </xs:all>
</xs:complexType>

<xs:complexType name="cluster-session-replicationType">
  <xs:all>
    <xs:element name="enabled" type="xs:boolean" default="true" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit/>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="port" type="portType" default="1099" minOccurs="0" maxOccurs="1">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit>1099</appinfo:implicit>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="key" type="xs:string" default="" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit/>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="encrypted" type="xs:boolean" default="true" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit>false</appinfo:implicit>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="cipher" type="cipherType" minOccurs="0" maxOccurs="1">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit>AES/CBC/PKCS5Padding</appinfo:implicit>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="protocol" type="sessionReplicationProtocolType" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit>JRMP</appinfo:implicit>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="async" type="xs:boolean" default="false" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit/>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="getAttribute-triggers-replication" type="xs:boolean" default="true" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit/>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="replica-discovery-max-hops" type="maxHopsType" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit>-1</appinfo:implicit>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

    <xs:element name="startup-discovery-timeout" type="intervalType" minOccurs="0"/>

    <xs:element name="cookie-name" type="noWhitespaceTokenType" minOccurs="0"/>
  </xs:all>
</xs:complexType>

<xs:simpleType name="maxHopsType">
  <xs:annotation>
    <xs:documentation>
      The value must be a maximum number of hops between 1 and 2147483647, inclusive. -1 indicates there is no maximum.
    </xs:documentation>
  </xs:annotation>
  <xs:union>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="2147483647"/>
      </xs:restriction>
    </xs:simpleType>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="-1"/>
        <xs:maxInclusive value="-1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:union>
</xs:simpleType>

<xs:complexType name="instance-session-replicationType">
  <xs:all>
    <xs:element name="enabled" type="xs:boolean" default="true" minOccurs="0">
      <xs:annotation>
        <xs:appinfo>
          <appinfo:implicit/>
        </xs:appinfo>
      </xs:annotation>
    </xs:element>

